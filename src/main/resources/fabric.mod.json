{
  "schemaVersion": 1,
  "id": "ramreducer",
  "version": "${version}",
  "name": "RAM Reducer",
  "description": "A mod that reduces RAM usage of other mods by optimizing memory allocation and lazy loading assets.",
  "authors": [
    "Manus"
  ],
  "contact": {
    "homepage": "https://manus.im",
    "sources": "https://github.com/manus/ram-reducer"
  },
  "license": "MIT",
  "icon": "assets/ramreducer/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.manus.ramreducer.RamReducer"
    ],
    "preLaunch": [
      "com.manus.ramreducer.RamReducerPreLaunch"
    ]
  },
  "mixins": [
    "ramreducer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~1.20",
    "java": ">=17",
    "fabric-api": "*"
  }
}
