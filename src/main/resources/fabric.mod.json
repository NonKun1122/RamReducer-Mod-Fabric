{
  "schemaVersion": 1,
  "id": "ramreducer",
  "version": "${version}",
  "name": "RAM Reducer",
  "description": "A mod that reduces RAM usage of other mods by optimizing memory allocation and lazy loading assets.",
  "authors": [
    "Manus"
  ],
  "contact": {
    "youTube": "https://youtube.com/@nonkung_ch?si=g_E10QsTq62Vx_EK",
    "modrinth": "https://modrinth.com/user/NonKungCh"
  },
  "license": "MIT",
  "icon": "assets/ramreducer/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.nonkungch.ramreducer.RamReducer"
    ],
    "preLaunch": [
      "com.nonkungch.ramreducer.RamReducerPreLaunch"
    ]
  },
  "mixins": [
    "ramreducer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~1.20",
    "java": ">=17",
    "fabric-api": "*"
  }
}
